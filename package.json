{
	"name": "game-input",
	"version": "0.0.1",
	"description": "",
	"main": "build/pkg/index.js",
	"module": "src/index.js",
	"unpkg": "build/pkg/game-input.min.js",
	"scripts": {
		"lint": "npm run lint:js; npm run lint:css",
		"lint:js": "eslint ./; true",
		"lint:css": "stylelint 'examples/**/*.css' 'tools/**/*.css'; true",
		"lint:fix": "eslint --fix ./; stylelint 'examples/**/*.css' 'tools/**/*.css' --fix; true",
		"prepush": "npm test",
		"start": "cross-env NODE_ENV=development webpack serve --config ./config/webpack-dev.js --mode development --progress",
		"build:cjs": "rm -rf build/cjs/ && cross-env NODE_ENV=production BABEL_ENV=cjs babel --config-file ./babel.config.js ./src --out-dir ./build/cjs --ignore \"**/*.test.js\"",
		"build:es": "rm -rf build/es/ && cross-env NODE_ENV=production BABEL_ENV=es babel --config-file ./babel.config.js ./src --out-dir ./build/es --ignore \"**/*.test.js\"",
		"build:esm": "rm -rf build/esm/ && cross-env NODE_ENV=production BABEL_ENV=esm babel --config-file ./babel.config.js ./src --out-dir ./build/esm --ignore \"**/*.test.js\"",
		"build:site:dev": "cross-env NODE_ENV=development rm -rf build/site/ && webpack --config ./config/webpack-dev.js --mode development",
		"build:site": "cross-env NODE_ENV=production rm -rf build/site/ && webpack --config ./config/webpack-prod.js --mode production",
		"build:lib": "npm run build:cjs && npm run build:esm && npm run build:es",
		"build": "npm run build:lib && npm run build:site",
		"deploy": "gh-pages -d ./build/site",
		"test": "cross-env NODE_ENV=test jest --verbose --coverage",
		"test:debug": "cross-env NODE_ENV=test node --inspect node_modules/.bin/jest --runInBand",
		"test:watch:debug": "cross-env NODE_ENV=test node --inspect node_modules/.bin/jest --runInBand --watch",
		"test:watch": "cross-env NODE_ENV=test jest --watch"
	},
	"author": "Brian Chirls <brian@chirls.com>",
	"license": "UNLICENSED",
	"dependencies": {
		"mitt": "^2.1.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.12.10",
		"@babel/core": "^7.12.10",
		"@babel/plugin-proposal-class-properties": "^7.12.1",
		"@babel/plugin-proposal-object-rest-spread": "^7.12.1",
		"@babel/plugin-syntax-dynamic-import": "^7.8.3",
		"@babel/plugin-transform-object-assign": "^7.12.1",
		"@babel/plugin-transform-runtime": "^7.12.10",
		"@babel/polyfill": "^7.12.1",
		"@babel/preset-env": "^7.12.11",
		"autoprefixer": "^10.2.5",
		"babel-eslint": "^10.1.0",
		"babel-jest": "^26.6.3",
		"babel-loader": "^8.2.2",
		"babel-plugin-module-resolver": "^4.1.0",
		"case-sensitive-paths-webpack-plugin": "^2.3.0",
		"clean-webpack-plugin": "^3.0.0",
		"cross-env": "^7.0.3",
		"css-loader": "^5.2.0",
		"css-minimizer-webpack-plugin": "^1.3.0",
		"datavized-code-style": "github:datavized/code-style",
		"eslint": "^7.16.0",
		"eslint-config-crockford": "^2.0.0",
		"eslint-loader": "^4.0.2",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-jest": "^24.1.3",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-promise": "^4.2.1",
		"fast-async": "^7.0.6",
		"file-loader": "^6.2.0",
		"gh-pages": "^3.1.0",
		"html-webpack-plugin": "^5.3.1",
		"jest": "^26.6.3",
		"jest-runner-eslint": "^0.10.0",
		"mini-css-extract-plugin": "^1.3.3",
		"postcss": "^8.2.2",
		"postcss-flexbugs-fixes": "^5.0.2",
		"postcss-input-range": "^4.0.0",
		"postcss-loader": "^4.1.0",
		"purecss": "^2.0.3",
		"raw-loader": "^4.0.2",
		"react-dev-utils": "^11.0.4",
		"stylelint": "^13.8.0",
		"stylelint-config-standard": "^21.0.0",
		"stylelint-webpack-plugin": "^2.1.1",
		"svgo": "^2.3.0",
		"svgo-loader": "^3.0.0",
		"url-loader": "^4.1.1",
		"webpack": "^5.28.0",
		"webpack-bundle-analyzer": "^4.4.0",
		"webpack-cli": "^4.6.0",
		"webpack-dev-server": "^3.11.0",
		"webpack-merge": "^5.7.3",
		"webpack-node-externals": "^2.5.2"
	}
}
