{
	"name": "game-input",
	"version": "0.0.1",
	"description": "",
	"main": "build/pkg/index.js",
	"module": "src/index.js",
	"unpkg": "build/pkg/game-input.min.js",
	"scripts": {
		"lint": "npm run lint:js; npm run lint:css",
		"lint:js": "eslint ./; true",
		"lint:css": "stylelint 'examples/**/*.css' 'tools/**/*.css'; true",
		"lint:fix": "eslint --fix ./; stylelint 'examples/**/*.css' 'tools/**/*.css' --fix; true",
		"prepush": "npm test",
		"start": "cross-env NODE_ENV=development webpack-dev-server --config ./config/webpack-dev.js --mode development --progress --colors",
		"build:dev": "cross-env NODE_ENV=development rm -rf build/site/ && webpack --config ./config/webpack-dev.js --mode development",
		"build": "cross-env NODE_ENV=production rm -rf build/site/ && webpack --config ./config/webpack-prod.js --mode production",
		"build:lib": "npm run build:cjs && npm run build:esm && npm run build:es",
		"build:cjs": "rm -rf build/cjs/ && cross-env NODE_ENV=production BABEL_ENV=cjs babel --config-file ./babel.config.js ./src --out-dir ./build/cjs --ignore \"**/*.test.js\"",
		"build:esm": "rm -rf build/esm/ && cross-env NODE_ENV=production BABEL_ENV=esm babel --config-file ./babel.config.js ./src --out-dir ./build/esm --ignore \"**/*.test.js\"",
		"build:es": "rm -rf build/es/ && cross-env NODE_ENV=production BABEL_ENV=es babel --config-file ./babel.config.js ./src --out-dir ./build/es --ignore \"**/*.test.js\"",
		"deploy": "gh-pages -d ./build/site",
		"test": "cross-env NODE_ENV=test jest --verbose --coverage",
		"test:debug": "cross-env NODE_ENV=test node --inspect node_modules/.bin/jest --runInBand",
		"test:watch:debug": "cross-env NODE_ENV=test node --inspect node_modules/.bin/jest --runInBand --watch",
		"test:watch": "cross-env NODE_ENV=test jest --watch"
	},
	"author": "Brian Chirls <brian@chirls.com>",
	"license": "UNLICENSED",
	"devDependencies": {
		"@babel/cli": "^7.10.3",
		"@babel/core": "^7.10.3",
		"@babel/plugin-proposal-class-properties": "^7.10.1",
		"@babel/plugin-proposal-object-rest-spread": "^7.10.3",
		"@babel/plugin-syntax-dynamic-import": "^7.8.3",
		"@babel/plugin-transform-object-assign": "^7.10.3",
		"@babel/plugin-transform-runtime": "^7.10.3",
		"@babel/polyfill": "^7.10.1",
		"@babel/preset-env": "^7.10.3",
		"autoprefixer": "^9.8.2",
		"babel-eslint": "^10.1.0",
		"babel-jest": "^26.0.1",
		"babel-loader": "^8.1.0",
		"babel-plugin-module-resolver": "^4.0.0",
		"case-sensitive-paths-webpack-plugin": "^2.3.0",
		"clean-webpack-plugin": "^3.0.0",
		"cross-env": "^7.0.2",
		"css-loader": "^3.5.3",
		"datavized-code-style": "github:datavized/code-style",
		"eev": "^0.1.5",
		"eslint": "^7.3.0",
		"eslint-config-crockford": "^2.0.0",
		"eslint-loader": "^4.0.2",
		"eslint-plugin-import": "^2.21.1",
		"eslint-plugin-jest": "^23.16.0",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-promise": "^4.2.1",
		"fast-async": "^7.0.6",
		"file-loader": "^6.0.0",
		"gh-pages": "^3.1.0",
		"html-webpack-plugin": "^4.3.0",
		"jest": "^26.0.1",
		"jest-runner-eslint": "^0.9.0",
		"mini-css-extract-plugin": "^0.9.0",
		"optimize-css-assets-webpack-plugin": "^5.0.3",
		"postcss-flexbugs-fixes": "^4.2.1",
		"postcss-input-range": "^4.0.0",
		"postcss-loader": "^3.0.0",
		"purecss": "^2.0.3",
		"raw-loader": "^4.0.1",
		"react-dev-utils": "^10.2.1",
		"stylelint": "^13.6.1",
		"stylelint-config-standard": "^20.0.0",
		"stylelint-webpack-plugin": "^2.1.0",
		"svgo": "^1.3.2",
		"svgo-loader": "^2.2.1",
		"url-loader": "^4.1.0",
		"webpack": "^4.43.0",
		"webpack-bundle-analyzer": "^3.8.0",
		"webpack-cli": "^3.3.12",
		"webpack-dev-server": "^3.11.0",
		"webpack-merge": "^4.2.2",
		"webpack-node-externals": "^1.7.2"
	}
}
